  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "axios": "^0.18.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.6",
    "nodemon": "^1.15.1",
    "typeface-nunito": "0.0.54"
  }
